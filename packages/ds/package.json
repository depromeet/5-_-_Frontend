  "description": "tocks tocks 디자인 시스템",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "sideEffects": false,
  "publishConfig": {
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "esm/index.js"
  },
  "scripts": {
    "build": "yarn clean && concurrently \"tsc -p tsconfig.json\" \"tsc -p tsconfig.esm.json\"",
